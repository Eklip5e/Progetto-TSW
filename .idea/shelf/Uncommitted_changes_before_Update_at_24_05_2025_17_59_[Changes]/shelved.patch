<+>* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n    font-family: 'Montserrat', sans-serif;\r\n    background-color: #24252A;\r\n    color: white;\r\n}\r\n\r\n/* ------------------------- */\r\n/* HEADER NAVBAR             */\r\n/* ------------------------- */\r\n.navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 15px 5%;\r\n    background-color: #24252A;\r\n}\r\n\r\n.logo img {\r\n    width: 50px;\r\n    height: 50px;\r\n}\r\n\r\n.nav_links {\r\n    list-style: none;\r\n    display: flex;\r\n    gap: 20px;\r\n}\r\n\r\n.nav_links li a {\r\n    color: #edf0f1;\r\n    font-weight: 800;\r\n    font-size: 16px;\r\n    text-decoration: none;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.nav_links li a:hover {\r\n    color: #9B72CF;\r\n}\r\n\r\n.nav_links i {\r\n    margin-right: 8px;\r\n}\r\n\r\n.header-icons {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 20px;\r\n}\r\n\r\n.header-icons a i {\r\n    font-size: 30px;\r\n    color: #edf0f1;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.header-icons a i:hover {\r\n    color: #9B72CF;\r\n}\r\n\r\n/* ------------------------- */\r\n/* GAME BANNER               */\r\n/* ------------------------- */\r\n.game-banner {\r\n    position: relative;\r\n    background-size: cover;\r\n    background-position: center;\r\n    height: 400px;\r\n    padding: 150px 10%;\r\n    text-align: left;\r\n}\r\n\r\n\r\n.update-button {\r\n    position: absolute;\r\n    top: 25px;\r\n    right: 25px;\r\n    background-color: #6b7280; /* grigio moderno */\r\n    color: white;\r\n    border: none;\r\n    padding: 10px 15px;\r\n    border-radius: 8px;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n    transition: background-color 0.2s ease;\r\n    z-index: 2; /* Assicura che stia sopra eventuali altri contenuti */\r\n}\r\n\r\n.update-button:hover {\r\n    background-color: #4b5563; /* grigio scuro */\r\n}\r\n\r\n.banner-content h1 {\r\n    font-size: 50px;\r\n    font-weight: 700;\r\n    margin-bottom: 15px;\r\n}\r\n\r\n.price-row {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 15px;\r\n}\r\n\r\n.discount-tag,\r\n.card-price-row .discount-tag {\r\n    background-color: #9B72CF;\r\n    color: white;\r\n    padding: 6px 12px;\r\n    border-radius: 8px;\r\n    font-weight: 400;\r\n    font-size: 15px;\r\n}\r\n\r\n.price {\r\n    font-size: 35px;\r\n    font-weight: 700;\r\n    color: white;\r\n}\r\n\r\n.card-price-row .price {\r\n    font-size: 20px;\r\n    font-weight: 700;\r\n    color: white;\r\n}\r\n\r\n/* ------------------------- */\r\n/* GAME GRID */\r\n/* ------------------------- */\r\n.game-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fit, 300px);\r\n    justify-content: center;\r\n    gap: 20px;\r\n    max-width: 1400px;\r\n    margin: 50px auto;\r\n}\r\n\r\n.game-card {\r\n    position: relative;\r\n    width: 100%;\r\n    max-width: 300px;\r\n    background-color: #2f3136;\r\n    border-radius: 16px;\r\n    overflow: hidden;\r\n    text-align: center;\r\n    padding-bottom: 20px;\r\n    transition: transform 0.2s ease;\r\n}\r\n\r\n.delete-button {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n    background-color: red;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 50%;\r\n    width: 24px;\r\n    height: 24px;\r\n    font-size: 18px;\r\n    line-height: 24px;\r\n    text-align: center;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n    padding: 0;\r\n    transition: background-color 0.2s;\r\n}\r\n\r\n.delete-button:hover {\r\n    background-color: darkred;\r\n}\r\n\r\n.game-card:hover {\r\n    transform: scale(1.03);\r\n}\r\n\r\n.game-card img {\r\n    width: 100%;\r\n    height: auto;\r\n    display: block;\r\n    object-fit: cover;\r\n}\r\n\r\n.game-card h2 {\r\n    font-size: 20px;\r\n    margin: 20px 0 20px;\r\n}\r\n\r\n.card-price-row {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    gap: 15px;\r\n}\r\n\r\n.add-game-card {\r\n    border: 5px dashed white;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    color: white;\r\n}\r\n\r\n/* Simbolo + grande */\r\n.add-game-card .plus-sign {\r\n    font-size: 48px;\r\n    font-weight: bold;\r\n}\r\n\r\n/* Testo sotto il + */\r\n.add-game-card p {\r\n    font-size: 20px;\r\n}\r\n\r\n/* Modale - overlay */\r\n.modal {\r\n    display: flex; /* permette centratura */\r\n    justify-content: center; /* centra orizzontalmente */\r\n    align-items: center;     /* centra verticalmente */\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background-color: rgba(0, 0, 0, 0.7);\r\n    z-index: 1000;\r\n}\r\n\r\n/* Contenuto modale - stile simile a game-card */\r\n.modal-content {\r\n    background-color: #2f3136;\r\n    border-radius: 16px;\r\n    padding: 30px;\r\n    color: white;\r\n    max-width: 400px;\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n    position: relative;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n/* Bottone per chiudere */\r\n.modal-content .close {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 15px;\r\n    font-size: 24px;\r\n    color: white;\r\n    cursor: pointer;\r\n}\r\n\r\n/* Input e textarea */\r\n.modal-content input,\r\n.modal-content textarea {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin: 8px 0;\r\n    border-radius: 8px;\r\n    border: none;\r\n    background-color: #40444b;\r\n    color: white;\r\n    font-size: 1rem;\r\n    outline: none;\r\n}\r\n\r\n/* Pulsante submit */\r\n.modal-content button {\r\n    width: 100%;\r\n    padding: 12px;\r\n    margin-top: 10px;\r\n    border: none;\r\n    border-radius: 10px;\r\n    background-color: #9B72CF;\r\n    color: white;\r\n    font-weight: bold;\r\n    font-size: 1rem;\r\n    cursor: pointer;\r\n    transition: background-color 0.2s ease;\r\n}\r\n\r\n.modal-content button:hover {\r\n    background-color: #9B72CF;\r\n}\r\n\r\n.error-message {\r\n    color: white;\r\n    padding: 10px;\r\n    margin-bottom: 15px;\r\n    text-align: center;\r\n}\r\n\r\n/* ------------------------- */\r\n/* REDIRECTS */\r\n/* ------------------------- */\r\n.signup-redirect form,\r\n.login-redirect form {\r\n    font-size: 12px;\r\n    color: #ccc;\r\n    position: relative;\r\n    bottom: 10px;\r\n    left: 10px;\r\n    text-decoration: none;\r\n}\r\n\r\n.signup-redirect:hover,\r\n.login-redirect:hover {\r\n    color: #9B72CF;\r\n    text-decoration: underline;\r\n}\r\n\r\n/* ------------------------- */\r\n/* FOOTER */\r\n/* ------------------------- */\r\n.footer {\r\n    background-color: #24252A;\r\n    padding: 40px 10%;\r\n    text-align: center;\r\n}\r\n\r\n.footer-content {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.footer-left {\r\n    font-size: 0.9rem;\r\n}\r\n\r\n.footer-right .social-links {\r\n    list-style: none;\r\n    display: flex;\r\n    gap: 15px;\r\n}\r\n\r\n.footer-right .social-links li a {\r\n    color: #edf0f1;\r\n    font-size: 20px;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.footer-right .social-links li a:hover {\r\n    color: #9B72CF;\r\n}\r\n\r\n/* ------------------------- */\r\n/* RESPONSIVE DESIGN */\r\n/* ------------------------- */\r\n@media (max-width: 768px) {\r\n    header.navbar {\r\n        flex-direction: column;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    .nav_links {\r\n        flex-direction: column;\r\n        gap: 10px;\r\n        margin-top: 10px;\r\n    }\r\n\r\n    .game-banner {\r\n        padding: 80px 5%;\r\n    }\r\n\r\n    .banner-content h1 {\r\n        font-size: 32px;\r\n    }\r\n\r\n    .price-row {\r\n        flex-direction: column;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    .game-grid {\r\n        padding: 30px 5%;\r\n        gap: 30px;\r\n    }\r\n\r\n    .footer-content {\r\n        flex-direction: column;\r\n        text-align: center;\r\n        gap: 20px;\r\n    }\r\n}
